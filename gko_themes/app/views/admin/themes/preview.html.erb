<html class=" js no-touch">
<head>
  <meta charset="utf-8">
  <title>Theme Roller Example Page</title>
  <!-- Set the viewport width to device width for mobile -->
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= stylesheet_link_tag "public" %>
</head>
<body id="home" style="display: block; ">
<div id="wrapper-wide-body">
  <!-- Header
        ================================================== -->
  <div class="navbar">
    <div class="navbar-inner">
      <div class="row-fluid">
        <div class="header-top hide-on-mobile">
          <div class="header-top-inner"></div>
        </div>
        <div class="header-nav">
          <!-- .btn-navbar is used as the toggle for collapsed navbar content -->
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <!-- Be sure to leave the brand out there if you want it shown -->
          <h2 id="site-title" class="brand"><a href="http://joufdesign.com/">Jouf Design</a></h2>
          <h4 id="site-subtitle" class="hide-on-mobile"></h4>
          <!-- Everything you want hidden at 940px or less, place within here -->
          <div class="nav-collapse">
            <!-- .nav, .navbar-search, .navbar-form, etc -->
            <nav id="primary-menu" class="primary-menu-container">
              <ul class="nav">
                <li><a href="#">
                  <div class="inner"><span class="icon"></span>

                    <div class="text">Page one</div>
                  </div>
                </a></li>
                <li>
                  <a href="/metiers">
                    <div class="inner"><span class="icon"></span>

                      <div class="text">Page two active</div>
                    </div>
                  </a>
                </li>
                <li>
                  <a href="/blog-billets">
                    <div class="inner"><span class="icon"></span>

                      <div class="text">Billets</div>
                    </div>
                  </a>
                </li>
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">Dropdown <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li><a href="#">Action</a></li>
                    <li><a href="#">Another action</a></li>
                    <li><a href="#">Something else here</a></li>
                    <li class="divider"></li>
                    <li><a href="#">Separated link</a></li>
                  </ul>
                </li>
              </ul>
            </nav>
            <nav id="pipe-menu" class="pipe-menu-container show-on-mobile">
              <ul class="nav">
                <li><a href="#">
                  <div class="inner"><span class="icon"></span>

                    <div class="text">Link one</div>
                  </div>
                </a></li>
                <li><a href="#">
                  <div class="inner"><span class="icon"></span>

                    <div class="text">Link two</div>
                  </div>
                </a></li>
                <li><a href="#">
                  <div class="inner"><span class="icon"></span>

                    <div class="text">Link three</div>
                  </div>
                </a></li>
              </ul>
            </nav>
          </div>
        </div>
      </div>

      <div id="content-container" class="container-fluid">
        <div id="content" class="row-fluid">
          <div id="main-column" class="span12 home ">
            <div class="main-column-inner">
              <div id="flash"></div>
              <div class="row-fluid">
                <div class="span12" id="home-features">
                  <div class="html_text"></div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- Footer ================================================== -->
      <div id="footer-container" class="container-fluid hide-on-mobile">
        <div id="footer" class="row-fluid">
          <div class="span12">
            <div class="footer-row-inner">
              <nav id="pipe-menu" class="pipe-menu-container hide-on-mobile">
                <ul class="nav">
                  <li><a href="#">
                    <div class="inner"><span class="icon"></span>

                      <div class="text">Link one</div>
                    </div>
                  </a></li>
                  <li><a href="#">
                    <div class="inner"><span class="icon"></span>

                      <div class="text">Link two</div>
                    </div>
                  </a></li>
                  <li><a href="#">
                    <div class="inner"><span class="icon"></span>

                      <div class="text">Link three</div>
                    </div>
                  </a></li>
                </ul>
              </nav>
            </div>
          </div>
        </div>
      </div>
      <div id="copyright-container" class="container-fluid">
        <div id="copyright" class="row-fluid">
          <div class="span12">
            <a href="/pages/1?mobile=true" id="device_switcher">Mobile version</a>
            <a id="site_by" href="http://www.joufdesign.com" target="_blank">Site par Jouf Design</a>
          </div>
        </div>
      </div>
    </div>
    <%= javascript_include_tag "gko_public_all" %>
